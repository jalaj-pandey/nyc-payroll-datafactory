{
	"name": "df_titleMaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "gen2_titleMaster_ds",
						"type": "DatasetReference"
					},
					"name": "srcTitleMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sql_titleMaster_ds",
						"type": "DatasetReference"
					},
					"name": "sqlTitleMaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as integer,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcTitleMaster",
				"srcTitleMaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as long,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sqlTitleMaster"
			]
		}
	}
}